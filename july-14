Public Class Class1
    Public Function add(n1 As Integer, n2 As Integer)
        Return n1 + n2
    End Function
    Public Function subtract(n1 As Integer, n2 As Integer)
        Return n1 - n2
    End Function
    Public Function multiply(n1 As Integer, n2 As Integer)
        Return n1 * n2
    End Function
    Public Function divide(n1 As Integer, n2 As Integer)
        Return n1 \ n2
    End Function
End Class
